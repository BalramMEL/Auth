generator client {
  provider = "prisma-client-js"
}

// prisma/schema.prisma
datasource db {
  provider  = "postgresql"
  url  	    = env("DATABASE_URL")
  
  directUrl = env("DATABASE_URL_UNPOOLED")
}

model User{
  id String @id @default(cuid())
  name String
}